datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model Item {
    id       Int      @id @default(autoincrement())
    date     DateTime
    category String
    title    String
    value    Float
}

model Category {
    tag     String   @id
    title   String
    color   String
    expense Boolean
}
